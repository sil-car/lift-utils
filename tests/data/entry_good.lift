    <sense id="c71333e0-be6c-434b-adda-bdc6c154d562">
        <grammatical-info value="n"/>
        <gloss lang="en">
            <text>grove</text>
        </gloss>
        <gloss lang="id">
            <text>dusun</text>
        </gloss>
        <example source="d2.077.03">
            <form lang="und-Latn">
                <text>Kbwai abatke to ksweruk nurare.</text>
            </form>
            <translation>
                <form lang="en">
                    <text>I went to the coconut groves to clear the grass.</text>
                </form>
                <form lang="id">
                    <text>Saya pergi menyiangi dusun kelapa.</text>
                </form>
            </translation>
        </example>
        <example source="d4.079.16">
            <form lang="und-Latn">
                <text>Kbwa ti ktwan nurke o abatke.</text>
            </form>
            <translation>
                <form lang="en">
                    <text>I'm going to plant coconut trees in the limited to coconut groves but is used for grove.</text>
                </form>
                <form lang="id">
                    <text>Saya pergi tanam kelapa di dusun.</text>
                </form>
            </translation>
        </example>
        <note type="encyclopedic">
            <form lang="en">
                <text>This is <span class="underline">not</span> limited to coconut groves but is used for mangoes, etc.</text>
            </form>
        </note>
    </sense>
